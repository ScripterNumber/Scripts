Tabs.MainTab:AddButton({
        Title = "Collect the gixtboxes Task",
        Description = "",
        Callback = function()

              if isExist() then
    -- Collect the giftboxes!
local HumanoidRootPart = GetHumanoidRootPart()
OldCharacterPosition = HumanoidRootPart.CFrame

    for _,v in pairs(workspace.GameTriggers.Items.GiftBoxes.ItemGroup:GetChildren()) do
       if v:IsA('MeshPart') then

             HumanoidRootPart.CFrame = CFrame.fromMatrix(
        v.Position + Vector3.new(0, 0, 0),
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.XVector,
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.YVector,
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.ZVector
        )

        wait(0.15)

        fireclickdetector(v.ClickDetector)

        
        wait(0.1)
       end
    end

    HumanoidRootPart.CFrame = workspace.GameTriggers.GiftboxesMiddle.GiftBox1.CFrame

    wait(0.2)

    HumanoidRootPart.CFrame = OldCharacterPosition

end

        end
    })

