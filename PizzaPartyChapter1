Tabs.MainTab:AddButton({
        Title = "Last Restore Power Task",
        Description = "",
        Callback = function()

              if isExist() then

local HumanoidRootPart = GetHumanoidRootPart()
OldCharacterPosition = HumanoidRootPart.CFrame
        if workspace.GameTriggers.Items.ItemGroup:FindFirstChild('Handle') then
        HumanoidRootPart.CFrame = CFrame.fromMatrix(
        workspace.GameTriggers.Items.ItemGroup.Handle.Position + Vector3.new(0, 0, 0),
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.XVector,
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.YVector,
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.ZVector
        )

        wait(0.15)
        
        fireclickdetector(workspace.GameTriggers.Items.ItemGroup.Handle.ClickDetector)
       
        wait(0.1)
         end
        
    for _,v in pairs(workspace.GameTriggers.Objectives.Panels:GetChildren()) do
       if v:IsA('MeshPart') and v:FindFirstChild('ClickDetector') then
        --xd
        HumanoidRootPart.CFrame = CFrame.fromMatrix(
        v.Position + Vector3.new(0, 0, 0),
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.XVector,
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.YVector,
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.ZVector
        )

        wait(0.15)

        fireclickdetector(v.ClickDetector)

        
        wait(0.1)
       end
    end

        
        wait(0.1)

        HumanoidRootPart.CFrame = OldCharacterPosition
end

        end
    })
